{
    "name": "ochoarobert1/wp-lemon-green",
    "description": "Lemon Green Services - Composer",
    "type": "project",
    "license": "GPL",
    "authors": [
        {
            "name": "Robert Ochoa",
            "email": "contacto@robertochoaweb.com"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "extra": {
        "installer-paths": {
            "source/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "source/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "require": {
        "wpackagist-plugin/elementor": "^3.20",
        "wpackagist-plugin/header-footer-elementor": "^1.6",
        "wpackagist-plugin/svg-support": "^2.5",
        "wpackagist-plugin/automatorwp": "^4.3",
        "wpackagist-plugin/performance-lab": "^2.9",
        "wpackagist-theme/hello-elementor": "^3.0",
        "wpackagist-plugin/wordpress-seo": "^22.4",
        "wpackagist-plugin/classic-editor": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require-dev": {
        "wpackagist-plugin/query-monitor": "^3.15"
    }
}
